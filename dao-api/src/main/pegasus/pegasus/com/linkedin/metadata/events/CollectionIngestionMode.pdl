namespace pegasus.com.linkedin.metadata.events

/**
 * this ingestion parameter represents how to process the ingestion of a collection aspect
 */
enum CollectionIngestionMode {
  UNKNOWN

  /**
   * append new items to the existing collection, using the defined update behavior to resolve duplicate conflicts
   */
  APPEND

  /**
   * remove items from an existing collection, using the defined primary key to determine if they exist within the collection
   */
  REMOVE
}