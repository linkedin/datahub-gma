namespace com.linkedin.metadata.backfill

/**
 * The mode of backfill. It's used to limit the scope of backfill process
 */
enum BackfillMode {

  /**
   * Backfill only SCSI
   */
  SCSI_ONLY

  /**
   * Backfill only using MAE
   */
  MAE_ONLY

  /**
   * Backfill only new entity tables
   */
  ENTITY_TABLES_ONLY

  /**
   * Backfill all secondary stores, excluding ENTITY_TABLES_ONLY
   */
  BACKFILL_ALL
}