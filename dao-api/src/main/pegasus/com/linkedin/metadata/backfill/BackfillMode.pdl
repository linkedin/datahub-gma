namespace com.linkedin.metadata.backfill

/**
 * The mode of backfill. It's used to limit the scope of backfill process
 */
enum BackfillMode {

  /**
   * To backfill only SCSI
   */
  SCSI_ONLY

  /**
   * To backfill only using MAE
   */
  MAE_ONLY

  /**
   * To backfill all secondary stores
   */
  BACKFILL_ALL
}