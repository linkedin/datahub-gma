namespace com.linkedin.metadata.query

/**
 * The additional metadata for a list result
 */
record ListResultMetadata {

  /**
   * A list that corresponds to soft deleted aspects
   */
  softDeletedAspects: array[ExtraInfo]

  /**
   * A list of extra information that positionally corresponds to the non-null aspects returned from the list query
   */
  extraInfos: array[ExtraInfo]
}